extends layout
include mixins

block content
    h1= title

    form(method='post', action='/addword')
        each field in form
            +field(field, submitted)
        button() Bæta við orði

    form(method='post', action='/getword')
        button() Ná í orð
    form(method='post', action='/updatelevel')
        button() Uppfæra stig